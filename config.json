{
  "queue": "queue.json",
  "wanted": ["x265", "h265", "hevc"],
  "extentions": [
    "mkv",
    "mp4"
  ],
  "ffprobe": "ffprobe -v error -select_streams v:0 -show_entries stream=codec_name -of default=noprint_wrappers=1:nokey=1 ",
  "ffmpeg": {
    "inargs": [
      "-y",
      "-progress -",
      "-nostats",
      "-loglevel info",
      "-nostdin"
    ],
    "outargs": [
      "-map 0:v:0 -c:v libx265",
      "-crf 24",
      "-map 0:a -c:a copy",
      "-map 0:s? -c:s copy"
    ]
  },
    "sleeptime": 5,
    "distributor": "http://localhost:3000"

}
